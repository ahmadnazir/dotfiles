{
    "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "workbench.editorAssociations": {
        "*.ts": "default",
        "*.ipynb": "jupyter-notebook"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.defaultInterpreterPath": "/home/mandark/code/scratch/python/env/bin/python3.9",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "nxConsole.enableTelemetry": false,
    "debug.javascript.autoAttachFilter": "disabled",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash",
            "args": [
                "-l"
            ]
        },
        "zsh": {
            "path": "zsh"
        },
        "fish": {
            "path": "fish"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "editor.formatOnSaveMode": "modifications",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": false,
        "typescript": true
    },
    "vim.commandLineModeKeyBindings": [],
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "scminput",
        "yaml",
        "magit"
    ],
    "workbench.sideBar.location": "right",
    "cursor.composer.renderPillsInsteadOfBlocks": true,

    "eslint.workingDirectories": [
        {
        "pattern": "*/"
        }
    ],
    "calva.paredit.defaultKeyMap": "strict",
    "files.autoSaveWorkspaceFilesOnly": true,
    "calva.evalOnSave": true,
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": false,
        "width": 120,
        "maxLength": 50
    },
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command"
    ],
    "window.commandCenter": true,
    "remote.SSH.remotePlatform": {
        "api.beamlynx": "linux",
        "dev-server": "linux",
        "rowl": "linux",
        "zen": "linux"
    },
    "files.exclude": {
        "config.ts": true
    },
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "cursor.general.globalCursorIgnoreList": [
        "**/.env",
        "**/.env.*",
        "**/credentials.json",
        "**/credentials.*.json",
        "**/secret.json",
        "**/secrets.json",
        "**/*.key",
        "**/*.pem",
        "**/*.pfx",
        "**/*.p12",
        "**/*.crt",
        "**/*.cer",
        "**/id_rsa",
        "**/id_dsa",
        "**/.ssh/id_*",
        "**/.cursor/mcp.json",
        "**/config.ts"
    ],
    "remote.autoForwardPortsSource": "hybrid"

}
